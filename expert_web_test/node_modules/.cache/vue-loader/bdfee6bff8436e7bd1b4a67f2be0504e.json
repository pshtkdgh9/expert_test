;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;AA27BA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA","file":"Integration.vue","sourceRoot":"src/components","sourcesContent":["<template>\r\n  <v-layout class=\"pa-1 layout d-flex flex-column pa-1\" >\r\n    <template class=\"d-flex\"  >\r\n<v-row class=\"ma-2 d-flex\">\r\n        <p class=\"text-left font-weight-bold text-h5\"  >\r\n          {{getIgName}}\r\n        </p>\r\n        <v-btn class=\"ml-2\" @click=\"setFilter()\" :disabled=\"filterDis\">필터 적용</v-btn>\r\n        <v-spacer></v-spacer>\r\n\r\n</v-row>\r\n\r\n<v-row>\r\n        <v-col cols=\"12\" md=\"5\" class=\"pl-8\">\r\n          <v-form v-model=\"valid\">\r\n\r\n          <v-text-field class=\"mx-4\" flat hide-details\r\n          label=\"Search\"\r\n          prepend-inner-icon=\"search\"\r\n          v-model=\"keywords\"\r\n          @keydown.enter=\"onSearch\"\r\n          :rules=\"searchWordRules\"\r\n          solo-inverted></v-text-field>\r\n          </v-form>\r\n        </v-col>\r\n        <v-col cols=\"4\" md=\"2\" class=\"mr-8\">\r\n\r\n          <v-btn class=\"mx-4\" color=\"success\" :disabled=\"!valid\"  @click=\"onSearch()\">검색</v-btn>\r\n\r\n          <v-btn class=\"mx-4\" color=\"primary\" @click=\"openDetailSearch()\">상세 검색</v-btn>\r\n\r\n          <v-dialog v-model=\"searchDialog\" persistent max-width=\"600px\">\r\n            <v-card>\r\n              <v-card-title>\r\n                <span class=\"headline\">검색 상세 설정</span>\r\n              </v-card-title>\r\n              <v-form v-model=\"valid\">\r\n                <v-card-text>\r\n\r\n                  <v-container>\r\n\r\n                    <template v-bind:props=\"searches\">\r\n                      <v-text-field :label=\"new Date().getFullYear()-years+ylabel\" required v-model=\"years\" :rules=\"numRules\" min=\"1\" max=\"30\" type=\"number\"></v-text-field>\r\n                      <v-row v-for=\"(w, i) in searches\" :key=\"i\">\r\n                        <v-col cols=\"10\" sm=\"10\" md=\"8\">\r\n                          <v-text-field label=\"검색어\" required v-model=\"w.word\" :rules=\"wordRules\"></v-text-field>\r\n                        </v-col>\r\n                        <v-col cols=\"2\" sm=\"2\" md=\"2\">\r\n                          <v-select label=\"연산\" :items=\"operations\" v-model=\"w.type\"></v-select>\r\n                        </v-col>\r\n                        <v-col cols=\"2\" sm=\"2\" md=\"2\">\r\n                          <v-btn  @click=\"removeKeyword(i)\">삭제\r\n                          </v-btn>\r\n                        </v-col>\r\n                      </v-row>\r\n                    </template>\r\n                    <v-row>\r\n                      <v-col cols=\"12\">\r\n                        <v-btn class=\"mx-2\" fab color=\"white\" @click=\"addSearchItem()\">\r\n                          <v-icon dark>add</v-icon>\r\n                        </v-btn>\r\n                      </v-col>\r\n                    </v-row>\r\n\r\n                  </v-container>\r\n                </v-card-text>\r\n                <v-card-actions>\r\n                  <v-spacer></v-spacer>\r\n                  <v-btn color=\"blue darken-1\" text @click=\"setKeywords()\" :disabled=\"!valid\">Save</v-btn>\r\n                </v-card-actions>\r\n              </v-form>\r\n            </v-card>\r\n          </v-dialog>\r\n        </v-col>\r\n\r\n\r\n\r\n<v-col cols=\"4\" md=\"2\" class=\"ml-10 pl-10\">\r\n          <v-btn color=\"#B3D4FB\" class=\"ml-6\" @click=\"openSiteDialog()\">사이트 설정</v-btn>\r\n          <!-- 사이트 설정 dialog (full) -->\r\n          <v-dialog v-model=\"siteDialog\" fullscreen hide-overlay transition=\"dialog-bottom-transition\">\r\n            <v-card>\r\n              <v-toolbar dark color=\"primary\">\r\n                <v-btn icon dark @click=\"closeSiteDialog()\">\r\n                  <v-icon>close</v-icon>\r\n                </v-btn>\r\n                <v-toolbar-title>Settings</v-toolbar-title>\r\n                <v-spacer></v-spacer>\r\n                <v-toolbar-items>\r\n                  <v-btn dark text @click=\"siteDialog = false\">Save</v-btn>\r\n                </v-toolbar-items>\r\n              </v-toolbar>\r\n\r\n              <v-list three-line subheader>\r\n                <v-subheader>Select Sites</v-subheader>\r\n                <v-list-item>\r\n                  <v-list-item-action>\r\n                    <v-checkbox type=\"checkbox\" v-model.lazy=\"selectAll\" label=\"모든 사이트\"></v-checkbox>\r\n                  </v-list-item-action>\r\n                </v-list-item>\r\n                <v-divider></v-divider>\r\n\r\n\r\n                <v-list-item v-for=\"(site, idx) in sites\" :key=\"idx\">\r\n                  <v-list-item-action>\r\n                    <template>\r\n                    <v-row>\r\n                      <v-col cols=\"7\">\r\n                        <v-checkbox v-model=\"siteselected\" :label=\"site.site\" :value=\"site\" :disabled=\"!site.canCrawl\"></v-checkbox>\r\n                      </v-col>\r\n\r\n                      <v-col cols=\"5\">\r\n                        <v-speed-dial\r\n                        v-show=\"(site.site === 'WOS' && site.canCrawl ) || (site.site === 'SCOPUS' && site.canCrawl )\"\r\n                        v-model=\"site.fab\"\r\n\r\n                       direction=\"right\"\r\n                       :open-on-hover=\"true\"\r\n                       transition=\"slide-x-transition\"\r\n                     >\r\n                     <template v-slot:activator>\r\n                       <v-btn\r\n                         color=\"blue darken-2\"\r\n                         dark\r\n                         fab\r\n                         x-small\r\n                         class=\"pa-5\"\r\n                         >\r\n                         {{filterMsg[site.filter]}}\r\n                       </v-btn>\r\n                     </template>\r\n                     <v-btn\r\n                       fab\r\n                       dark\r\n                       x-small\r\n                       color=\"green\"\r\n                       class=\"pa-5\"\r\n                       @click=\"site.filter=true\"\r\n                     >\r\n                     ALL\r\n                     </v-btn>\r\n                     <v-btn\r\n                       fab\r\n                       dark\r\n                       x-small\r\n                       color=\"indigo\"\r\n                       class=\"pa-5\"\r\n                       @click=\"site.filter=false\"\r\n                     >\r\n                       Korea\r\n                     </v-btn>\r\n                    </v-speed-dial>\r\n                  </v-col>\r\n                </v-row>\r\n                </template>\r\n              </v-list-item-action>\r\n            </v-list-item>\r\n\r\n          </v-list>\r\n        </v-card>\r\n      </v-dialog>\r\n        </v-col>\r\n        <v-spacer></v-spacer>\r\n        <v-menu\r\n        v-model=\"menu\"\r\n        :close-on-content-click=\"false\"\r\n        :nudge-width=\"100\"\r\n        offset-x\r\n        >\r\n        <template v-slot:activator=\"{ on, attrs }\">\r\n        <v-btn\r\n          color=\"indigo\"\r\n          class=\"ml-6\"\r\n          dark\r\n          v-bind=\"attrs\"\r\n          v-on=\"on\"\r\n\r\n        >\r\n          Ranking\r\n        </v-btn>\r\n        </template>\r\n            <v-card>\r\n              <v-list two-line>\r\n                <template v-for=\"(item, index) in rankings.slice(0, 6)\">\r\n                  <v-subheader\r\n                    v-if=\"item.header\"\r\n                    :key=\"item.header\"\r\n                  >\r\n                    {{ item.header }}\r\n                  </v-subheader>\r\n                  <v-divider\r\n                    v-else-if=\"item.divider\"\r\n                    :key=\"index\"\r\n                    :inset=\"item.inset\"\r\n                  ></v-divider>\r\n                  <v-list-item\r\n                    v-else\r\n                    :key=\"item.title\"\r\n                  >\r\n                    <v-list-item-content>\r\n                      <v-list-item-title v-html=\"item.title\"></v-list-item-title>\r\n                    </v-list-item-content>\r\n                  </v-list-item>\r\n                </template>\r\n              </v-list>\r\n            </v-card>\r\n          </v-menu>\r\n  </v-row>\r\n</template>\r\n\r\n<v-layout>\r\n  <v-row>\r\n    <!-- :options.sync=\"options\" -->\r\n      <v-col cols=\"2\" >\r\n        <!-- <v-expansion-panels\r\n      v-model=\"panel\"\r\n\r\n      multiple\r\n    > -->\r\n    <v-tabs v-model=\"tab\" fixed-tabs >\r\n         <v-tab style=\"width:100px\" class=\"text-subtitle-1\" v-for=\"(v, pp, idx) in getFilters\" :key=\"idx\">{{pp}} Filter</v-tab>\r\n\r\n          <v-tab-item v-for=\"(v, pp, idx) in getFilters\" :key=\"idx\">\r\n      <v-card>\r\n    <v-list\r\n        subheader\r\n\r\nclass=\"overflow-y-auto\"\r\n\r\n    max-height=\"300\"\r\n     v-for=\"(fil, idx) in v\" :key=\"idx\"\r\n\r\n   >\r\n\r\n   <v-subheader  style=\"position : sticky; top:0; z-index:1; background: #b3d4fc; background-color : #b3d4fc;\">{{fil.v}}</v-subheader>\r\n     <v-spacer></v-spacer>\r\n\r\n     <v-list-item-group\r\n\r\n      v-model=\"fil.selected\"\r\n       multiple\r\n     >\r\n     <v-list-item  v-for=\"(vv, kk, iidx) in fil.list\" :key=\"iidx\" active-class=\"text--accent-1\"  >\r\n     <template v-slot:default=\"{ active }\" class=\"overflow-y-auto\">\r\n           <v-list-item-content >\r\n             <v-list-item-title  style=\"font-size: 0.8em;\" class=\"text-left text-wrap\" >{{getValue(kk,vv,fil.k)}}</v-list-item-title>\r\n           </v-list-item-content>\r\n\r\n           <v-list-item-action>\r\n             <v-checkbox  :input-value=\"active\" ></v-checkbox>\r\n           </v-list-item-action>\r\n         </template>\r\n\r\n\r\n       <!-- <v-list-item v-for=\"(item, idx) in fil.list\" :key=\"idx\" active-class=\"text--accent-4\">\r\n         <template v-slot:default=\"{ active }\">\r\n           <v-list-item-action>\r\n             <v-checkbox v-model=\"selected\" :input-value=\"active\" :label=\"item\" :value=\"item\"></v-checkbox>\r\n           </v-list-item-action>\r\n         </template> -->\r\n       </v-list-item>\r\n     </v-list-item-group>\r\n     <v-divider></v-divider>\r\n\r\n   </v-list>\r\n\r\n   </v-card>\r\n </v-tab-item>\r\n\r\n\r\n</v-tabs>\r\n\r\n   </v-col>\r\n\r\n   <!-- :itemsPerPage=\"ipg\"\r\n   :expanded.sync=\"expanded\"\r\nshow-expand\r\n -->\r\n     <v-col cols=\"9\" class=\"pr-0 ml-5\">\r\n    <v-data-table\r\n        ref=\"vuetable\"\r\n        :headers=\"headers\"\r\n        :server-items-length=\"getTotal()\"\r\n        :options.sync=\"options\"\r\n        :expanded.sync=\"expanded\"\r\n        item-key=\"_id\"\r\n        :loading = \"loading\"\r\n        show-expand\r\n        class=\"elevation-1\"\r\n        @item-expanded=\"loadDetails\"\r\n        multi-line\r\n      >\r\n      <!-- <template v-slot:item.inst=\"{ item }\">\r\n<v-tooltip>\r\n        <template v-slot:activator=\"{ on, attrs }\">\r\n      <span\r\n        v-bind=\"attrs\"\r\n        v-on=\"on\"\r\n        class=\"text-decoration-underline\"\r\n      >소속</span>\r\n    </template>\r\n\r\n    <span>{{getInst(item)}}</span></v-tooltip>\r\n\r\n\r\n       </template> -->\r\n\r\n       <template v-slot:item.totalCoop=\"{ item }\">\r\n\r\n         <v-tooltip bottom v-if=\"parseInt(item.totalCoop) > 0\">\r\n        <template v-slot:activator=\"{ on, attrs }\">\r\n         <span\r\n         v-bind=\"attrs\"\r\n         v-on=\"on\"\r\n         class=\"text-decoration-underline\"\r\n         >{{item.totalCoop}}</span>\r\n         </template>\r\n\r\n         <span>{{makeCoopList(item.coopList)}}</span></v-tooltip>\r\n\r\n       </template>\r\n       <template v-slot:item.score=\"{ item }\">\r\n\r\n       <v-speed-dial\r\n      direction=\"left\"\r\n      :open-on-hover=\"true\"\r\n      transition=\"slide-x-transition\"\r\n    >\r\n\r\n    <template v-slot:activator>\r\n      <v-btn\r\n        color=\"blue darken-2\"\r\n        dark\r\n        fab\r\n        x-small\r\n        class=\"pa-5\"\r\n        >\r\n        {{item.score}}\r\n\r\n      </v-btn>\r\n    </template>\r\n\r\n    <v-btn\r\n      fab\r\n      dark\r\n      x-small\r\n      color=\"green\"\r\n      class=\"pa-5\"\r\n      style=\"width:65px\"\r\n\r\n    >\r\n      주제적합도  <br />{{getScoreString(item.factor.acc)}}\r\n\r\n    </v-btn>\r\n    <v-btn\r\n      fab\r\n      dark\r\n      x-small\r\n      color=\"indigo\"\r\n      class=\"pa-5\"\r\n\r\n    >\r\n      생산성  <br />{{getScoreString(item.factor.qunt)}}\r\n    </v-btn>\r\n    <v-btn\r\n      fab\r\n      dark\r\n      x-small\r\n      color=\"purple\"\r\n      class=\"pa-5\"\r\n\r\n    >\r\n  품질  <br />{{getScoreString(item.factor.qual)}}\r\n    </v-btn>\r\n    <v-btn\r\n      fab\r\n      dark\r\n      x-small\r\n      color=\"teal lighten-3\"\r\n      class=\"pa-5\"\r\n\r\n    >\r\n협업도  <br />{{getScoreString(item.factor.coop)}}\r\n\r\n    </v-btn>\r\n   </v-speed-dial>\r\n   </template>\r\n\r\n      <template v-slot:item.webSearch=\"{ item }\">\r\n        <a :href=\"item.webSearch\" target=\"_blank\">검색</a>\r\n       </template>\r\n\r\n\r\n       <template v-slot:expanded-item=\"{ item}\">\r\n         <td :colspan=\"headers.length\">\r\n    <v-list\r\n      subheader\r\n      three-line\r\n      style=\"width : 100%\"\r\n    >\r\n      <template v-for=\"(raw, _idx) in item.raw\">\r\n     <v-divider inset :key=\"raw.idx\"></v-divider>\r\n     <v-subheader inset :key=\"raw.site+raw.id\" v-if=\"_idx == 0 && raw.site == 'NTIS'\">연구 과제</v-subheader>\r\n     <v-subheader :key=\"raw.id\" inset v-if=\"(_idx == 0 && raw.site !='NTIS') ||( _idx!=0 && item.raw[_idx-1].site == 'NTIS' && raw.site !='NTIS')\">논문</v-subheader>\r\n     <v-list-item\r\n     :key=\"raw._id\"\r\n      >\r\n      <v-list-item-avatar>\r\n          {{raw.idx}}\r\n        </v-list-item-avatar>\r\n\r\n      <v-list-item-content v-if=\"raw.site=='NTIS'\">\r\n         <v-list-item-title  class=\"text-sm-left\">{{raw.koTitle}}\r\n          / {{ raw.odAgency }} / 연구책임자({{raw.mng}}) / {{transFund(raw.totalFund)}}\r\n         </v-list-item-title>\r\n\r\n         <v-list-item-subtitle class=\"text-sm-left\" >\r\n           과제키워드 : {{raw.koKeyword}} /\r\n           연구기간 : {{ raw.prdStart.slice(0,4)}} ~ {{raw.prdEnd.slice(0,4)}} /\r\n           <v-tooltip bottom>  <template v-slot:activator=\"{ on, attrs }\">\r\n        <span\r\n          v-bind=\"attrs\"\r\n          v-on=\"on\"\r\n          class=\"text-decoration-underline\"\r\n        >참여연구자</span>\r\n      </template>\r\n\r\n      <span>{{raw.rsc}}</span></v-tooltip> : {{(parseInt(raw.cntRscMan)+parseInt(raw.cntRscWom))}}명 /\r\n      <v-tooltip bottom>  <template v-slot:activator=\"{ on, attrs }\">\r\n      <span\r\n      v-bind=\"attrs\"\r\n      v-on=\"on\"\r\n      class=\"text-decoration-underline\"\r\n      >요약</span>\r\n      </template>\r\n\r\n      <span>{{raw.absAbs}}</span></v-tooltip> /\r\n      <v-btn text color=\"primary\" :href=\"kvs[raw.site].link+raw.id\" target=\"_blank\">\r\n          {{raw.site}}\r\n        </v-btn>\r\n         </v-list-item-subtitle>\r\n       </v-list-item-content>\r\n\r\n       <v-list-item-content v-if=\"raw.site!='NTIS'\">\r\n          <v-list-item-title  class=\"text-sm-left\">{{raw.title}} / {{raw.issue_inst}} / {{raw.issue_year}}\r\n          </v-list-item-title>\r\n\r\n          <v-list-item-subtitle class=\"text-sm-left\" >\r\n\r\n            저자 : {{makeAuthorName(raw.author, raw.originalName)}} / 키워드 : {{raw.paper_keyword}} /  인용 수 : {{raw.citation}} /\r\n            <v-tooltip bottom>\r\n              <template v-slot:activator=\"{ on, attrs }\">\r\n            <span\r\n              v-bind=\"attrs\"\r\n              v-on=\"on\"\r\n              class=\"text-decoration-underline\"\r\n            >요약</span>\r\n          </template>\r\n\r\n          <span>{{raw.abstract}}</span></v-tooltip>\r\n          / <v-btn text color=\"primary\" :href=\"kvs[raw.site].link+raw.id\" target=\"_blank\">\r\n            {{raw.site}}\r\n          </v-btn>\r\n          </v-list-item-subtitle>\r\n\r\n        </v-list-item-content>\r\n\r\n       <!-- <v-list-item-action>\r\n         <v-btn icon>\r\n           <v-icon color=\"grey lighten-1\">mdi-information</v-icon>\r\n         </v-btn>\r\n       </v-list-item-action> -->\r\n\r\n     </v-list-item>\r\n\r\n       </template>\r\n  </v-list>\r\n\r\n</td>\r\n\r\n\r\n<!-- <v-row>\r\n         <td :colspan=\"headers.length\">\r\n\r\n         <v-simple-table>\r\n\r\n               <template v-slot:default>\r\n                 <thead>\r\n                   <tr>\r\n                     <th>\r\n                       순번\r\n                     </th>\r\n                     <th>제목</th>\r\n                     <th>키워드</th>\r\n                     <th>공저자/참여연구원</th>\r\n                     <th>학회명/지원기관</th>\r\n                     <th>날짜</th>\r\n                     <th>요약</th>\r\n                     <th>웹 연결</th>\r\n                   </tr>\r\n                 </thead>\r\n                 <tbody>\r\n                   <tr v-for=\"raw in getIgRaw(item.idx)\" :key=\"raw.idx\">\r\n                      <td>{{ raw.idx }}</td>\r\n                     <td>{{raw[kvs[raw.site].title]}}</td>\r\n                     <td>{{raw[kvs[raw.site].keyword]}}</td>\r\n                     <td>{{raw[kvs[raw.site].author]}}</td>\r\n                     <td>{{ raw[kvs[raw.site].j] }}</td>\r\n                     <td>{{ raw[kvs[raw.site].y] }}</td>\r\n                     <td>요약보기</td>\r\n                     <td><a :href=\"kvs[raw.site].link+raw.id\" target=\"_blank\">{{raw.site}}</a></td>\r\n                   </tr>\r\n                 </tbody>\r\n               </template>\r\n             </v-simple-table>\r\n           </td>\r\n           </v-row> -->\r\n       </template>\r\n      </v-data-table>\r\n      </v-col>\r\n    </v-row>\r\n    </v-layout>\r\n  </v-layout>\r\n\r\n  <!-- <template v-slot:expanded-item=\"{ headers, item}\">\r\n\r\n    <td :colspan=\"headers.length\" >\r\n      asd\r\n    </td>\r\n  </template> -->\r\n\r\n\r\n\r\n\r\n</template>\r\n\r\n<script>\r\nexport default {\r\n  data() {\r\n    return {\r\n      tab : null,\r\n      loading : false,\r\n      dialog : false,\r\n      filterDis : true,\r\n      panel : [0, 1],\r\n      ylabel : \" 년 이후 논문 수집\",\r\n      years : 10,\r\n      siteDialog: false,\r\n      tempSelected : [],\r\n      siteselected: [],\r\n      sites: [\r\n        {'site' : 'NTIS', 'canCrawl' : true, 'filter' : true},\r\n        {'site' : 'SCIENCEON', 'canCrawl' : true , 'filter' : true},\r\n        {'site' : 'DBPIA', 'canCrawl' : true , 'filter' :  true},\r\n        {'site' : 'SCOPUS', 'canCrawl' : true, 'filter' : true, 'fab' : false},\r\n        {'site' : 'WOS', 'canCrawl' : false, 'filter' : true, 'fab' : false},\r\n        {'site' : 'KCI', 'canCrawl' : true, 'filter' :  true }\r\n      ],\r\n      rankings : [\r\n        { header: 'Ranking', sortable: true, align: 'center' },\r\n        { avatar: 'https://cdn.vuetifyjs.com/images/lists/1.jpg', title: '서울대학교', subtitle: `<span class=\"font-weight-bold\">Ali Connors</span> &mdash; I'll be in your neighborhood doing errands this weekend. Do you want to hang out?` },\r\n        { divider: true, inset: true },\r\n        { avatar: 'https://cdn.vuetifyjs.com/images/lists/2.jpg', title: '충북대학교', subtitle: `<span class=\"font-weight-bold\">to Alex, Scott, Jennifer</span> &mdash; Wish I could come, but I'm out of town this weekend.` },\r\n        { divider: true, inset: true },\r\n        { avatar: 'https://cdn.vuetifyjs.com/images/lists/3.jpg', title: '고려대학교', subtitle: '<span class=\"font-weight-bold\">Sandra Adams</span> &mdash; Do you have Paris recommendations? Have you ever been?' },\r\n      ],\r\n      kvs : {'NTIS' : {\r\n        \"link\" : 'https://www.ntis.go.kr/project/pjtInfo.do?pjtId=',\r\n        \"title\" : \"koTitle\",\r\n        \"keyword\" : \"koKeyword\",\r\n        \"author\" : \"rsc\",\r\n        \"j\" : \"odAgency\",\r\n        \"y\" : \"prdStart\"\r\n      },\r\n      'SCIENCEON' :{\r\n        \"link\" : 'https://scienceon.kisti.re.kr/srch/selectPORSrchArticle.do?cn=',\r\n    \"title\" : \"title\",\r\n    \"keyword\" : \"paper_keyword\",\r\n    \"author\" : \"author\",\r\n    \"j\" : \"journal\",\r\n    \"y\" : \"issue_year\"\r\n\r\n  }\r\n      , 'DBPIA' : {\r\n        \"link\":'https://www.dbpia.co.kr/journal/articleDetail?nodeId=',\r\n        \"title\" : \"title\",\r\n        \"keyword\" : \"paper_keyword\",\r\n          \"author\" : \"author\",\r\n            \"j\" : \"journal\",\r\n            \"y\" : \"issue_year\"\r\n      },\r\n      'KCI' :{\r\n'link' : 'https://www.kci.go.kr/kciportal/ci/sereArticleSearch/ciSereArtiView.kci?sereArticleSearchBean.artiId='\r\n}\r\n    },\r\n\r\n\r\n      expanded: [],\r\n      options : {},\r\n      initselected : {'project' : {'inst' : [], 'rsc' : [], 'fund' : [], 'year' : []},\r\n'paper' : {'inst' : [], 'year' : [], 'journal' : [], 'lang' : []},\r\n    },\r\n      mStr : {true : '숨기기' ,false : '더보기'},\r\n\r\n      selected : {'project' : {'inst' : [], 'rsc' : [], 'fund' : [], 'year' : []},\r\n'paper' : {'inst' : [], 'year' : [], 'journal' : [], 'lang' : []},\r\n    },\r\n      filterListProject : { \"i\" : {\"value\" : \"소속\", \"list\" : [] },\r\n\"y\" : {\"value\" : \"연도\", \"list\" : [] },\r\n\"s\" : {\"value\" : \"과제규모\", \"list\" : [] }\r\n},\r\nfilterListPaper : { \"j\" : {\"value\" : \"저널명\", \"list\" : [\"한국\", \"콘텐츠\"] },\r\n\"i\" : {\"value\" : \"소속\", \"list\" : [] },\r\n\"y\" : {\"value\" : \"연도\", \"list\" : [] },\r\n\"o\" : {\"value\" : \"협업 OEM\", \"list\" : [] },\r\n},\r\n      headers: [\r\n        {\r\n          text: 'No.',\r\n          align: 'start',\r\n          sortable: false,\r\n          value: 'idx',\r\n        },\r\n       {\r\n         text: '성명',\r\n         align: 'start',\r\n         sortable: true,\r\n         value: 'name',\r\n         witdth:'10%'\r\n       },\r\n       { text: '소속', sortable: true , align: 'center', value: 'inst', width:'10%'},\r\n       //{ text: '연구 키워드', sortable: false , align: 'center', value: 'rKeywords' },\r\n       { text: '(논문)논문 수', sortable: true , align: 'center',value: 'numPapers'},\r\n       { text: '(논문)피인용 합계', sortable: true , align: 'center',value: 'totalCitation'},\r\n       { text: '(논문)공동연구 건 수', sortable: true , align: 'center',value: 'totalCoop'},\r\n       { text: '(논문)최신 게재년도', sortable: true , align: 'center',value: 'recentYear'},\r\n       { text: '과제 수', sortable: true, align: 'center',value: 'numProjects'},\r\n       { text: 'NGV 협업 수', sortable: false , align: 'center',value: 'ngvCoop'},\r\n       { text: 'NGV 스코어', sortable: true , align: 'center',value: 'score'},\r\n       { text: '웹 검색', sortable: false , align: 'center',value: 'webSearch'},\r\n\r\n     ],\r\n     headers2: [\r\n       {\r\n         text: 'No.',\r\n         align: 'start',\r\n         sortable: false,\r\n         value: 'idx',\r\n       },\r\n      {\r\n        text: '제목',\r\n        align: 'start',\r\n        sortable: false,\r\n        value: 'title',\r\n      },\r\n      { text: '요2', sortable: false , align: 'center', value: 'abstract' },\r\n      { text: '연구 키워드', sortable: false , align: 'center', value: 'rKeywords' },\r\n      { text: '연구 실적', sortable: false , align: 'center',value: 'numPapers'},\r\n      { text: '협업 이력', sortable: true , align: 'center',value: 'coopHist'},\r\n      { text: '전문가 지수', sortable: true , align: 'center',value: 'score'},\r\n      { text: '네트워크', sortable: false , align: 'center',value: 'network'},\r\n      { text: '웹 검색', sortable: false , align: 'center',value: 'webSearch'},\r\n\r\n    ],\r\n      itemsPerPageArray: [5, 8, 12],\r\n      // filterStr : { : '(ALL)' ,  false : '(Korea)'}\r\n      page: 1,\r\n      itemsPerPage : 10,\r\n      first : true\r\n    }\r\n  },\r\n\r\n  watch: {\r\n    options: {\r\n      handler(){\r\n        if(this.first){\r\n          this.first = false;\r\n        }\r\n        else{\r\n          this.pageReload();\r\n          this.filterDis = false;\r\n        }\r\n        //console.log('THIS.PAGE', this.page);\r\n        //console.log('THIS.OPTIONS.PAGE', this.options.page);\r\n        // this.page = this.options.page;\r\n      },\r\n      deep: true\r\n    }\r\n  },\r\n\r\n  beforeDestroy() {\r\n    this.$store.state.integrations = [];\r\n    this.$store.state.igRaw = {};\r\n    this.$store.state.prevfId = 0;\r\n    this.$store.dispatch('deleteIntegration', this.$store.state.igID);\r\n    this.$store.commit('deleteSelected');\r\n    // remove filter data all (keyId)\r\n  },\r\n  created(){\r\n    // this.$store.dispatch('getHistories');\r\n    this.$store.commit(\"setIsMini\", true);\r\n\r\n    // this.$store.state.isMini\r\n    // console.log('THIS.$STORE.STATE.ISMINI', this.$store.state.isMini);\r\n\r\n  },\r\n  mounted(){\r\n      this.setFilter();\r\n  },\r\n  computed: {\r\n    selectAll: {\r\n      get: function () {\r\n        return this.sites ? this.selected.length === this.sites.filter(site=>site.canCrawl).length : false\r\n      },\r\n      set: function (value) {\r\n        // console.log('VALUE', value);\r\n        let selected = [];\r\n        if (value) {\r\n          selected = this.sites.filter(site=>site.canCrawl);\r\n        }\r\n        this.selected = selected;\r\n      }\r\n    },\r\n\r\n     getSelected(){\r\n      // get(){\r\n        return this.$store.state.selected;\r\n      },\r\n    // getSelected: {\r\n    //   get(){\r\n    //     return this.$store.state.selected;\r\n    //   },\r\n    //   set(value){\r\n    //     console.log('VALUE', value);\r\n    //     // this.$store.state.\r\n    //   }\r\n    //\r\n    // },\r\n    getIgName(){\r\n      return this.$store.state.igKeyword +\" ( KeyID : \"+this.$store.state.igID+\" )\";\r\n    },\r\n    getIgs() {\r\n      return this.$store.state.integrations;\r\n    },\r\n    numberOfPages() {\r\n      return Math.ceil(this.getHistories.length / this.itemsPerPage);\r\n    },\r\n    ipg(){\r\n      return this.itemsPerPage;\r\n    },\r\n    getFilters(){\r\n      return this.$store.state.filters;\r\n    },\r\n    isShowFilter(){\r\n      return (fil, index) =>{\r\n        return fil.f || index < 5;\r\n      };\r\n    },\r\n    getIgTotal(){\r\n      return this.$store.state.igTotal;\r\n    },\r\n    getIgRaw(){\r\n      return idx => this.$store.state.igRaw[idx];\r\n    }\r\n  },\r\n\r\n  methods: {\r\n    getScoreString(value){\r\n      if(value > 0.7)\r\n        return \"상\";\r\n      else if(value > 0.5)\r\n        return \"중\";\r\n      else\r\n        return \"하\";\r\n\r\n    },\r\n    /*\r\n    getInst(item){\r\n      let sites = ['NTIS', 'Scienceon', 'KCI'];\r\n      let rtv = \"\";\r\n      sites.forEach((site) => {\r\n        if(item[site] !== undefined) {\r\n          rtv += site+\":\"+item[site].inst+\"\\n\";\r\n        }\r\n      });\r\n\r\n      console.log('ITEM', item);\r\n      return rtv;\r\n\r\n    },\r\n    */\r\n    transFund(fund){\r\n      return fund/1000000  + '백만원';\r\n    },\r\n\r\n    // getLink(site, id){\r\n    //\r\n    // },\r\n    async loadDetails({item}){\r\n\r\n      if(this.getIgRaw(item.idx) === undefined){\r\n        // console.log('CONDITION PASSED');\r\n        // console.log('ITEM', item);\r\n        // item.raws = [{\"title\" : \"Test\"}];\r\n        // console.log(\"expand\");\r\n        // this.expanded\r\n        console.log('THIS.EXPANDED', this.expanded);\r\n        // let t = this;\r\n        this.expanded.filter(x=> x==item);\r\n\r\n\r\n      //   this.$store.dispatch('getIntegrationRaw', item).then(()=>{\r\n      //     t.expanded.push(item);\r\n      // });\r\n        // .then(res => {item.raws = res;\r\n        //   console.log('ITEM.RAWS', res);\r\n        //   console.log('  T.$REFS.VUETABLE',   t.$refs.vuetable);\r\n        //   // t.$refs.vuetable.refresh();\r\n        //   t.$forceUpdate();\r\n        // });\r\n      }else{\r\n          console.log('ITEM.RAWS', item);\r\n      }\r\n    },\r\n\r\n    getTotal(){\r\n      // console.log('THIS.GETIGS', this.getIgs);\r\n      if (this.getIgTotal >50)\r\n        return 50;\r\n      else\r\n        return this.getIgTotal\r\n    },\r\n    pageReload(){\r\n      let data = {};\r\n      data.keyId = this.$store.state.igID;\r\n      data.options = this.options;\r\n      data.fId = this.$store.state.prevfId;\r\n      this.loading = true;\r\n      let t = this;\r\n      this.$store.dispatch('getReloadPage', data).then(()=>{\r\n        t.loading = false;\r\n      });\r\n    },\r\n    setFilter(){\r\n      // this.selected\r\n\r\n      // this.$test\r\n      this.filterDis = true;\r\n      console.log('THIS.$TEST', this.$refs['test']);\r\n      console.log('THIS.SELECTED', this.selected);\r\n\r\n\r\n      // let data = Object.assign({}, this.selected);\r\n      console.log(\"Test Filter\");\r\n      // this.options\r\n      console.log('THIS.OPTIONS', this.options);\r\n\r\n      let data = {};\r\n      // console.log('IPG()', this.itemsPerPage);\r\n      this.fisrt = false;\r\n\r\n      data.keyId = this.$store.state.igID;\r\n      data.options = this.options;\r\n      data.options.page = 1;\r\n      let t = this;\r\n      // t.selected = t.initselected;\r\n      this.loading = true;\r\n      this.$store.dispatch('getIntegrationResult', data).then((sel) =>{\r\n        console.log('SEL', sel);\r\n        t.loading = false;\r\n        t.filterDis = false;\r\n        // sel.filters\r\n\r\n\r\n        // t.selected = sel.selected;\r\n        // console.log('SEL', sel.selected.paper);\r\n          // t.selected = sel;\r\n          // t.$forceUpdate()\r\n          // t.$refs['test'].$listeners.change();\r\n      });\r\n    },\r\n    setFlag(pp, k){\r\n      this.$store.state.filters[pp][k].f = !this.$store.state.filters[pp][k].f ;\r\n\r\n      // fil\r\n    },\r\n\r\n    refreshHistories() {\r\n      this.$store.dispatch('getHistories', 0)\r\n    },\r\n     onScroll () {\r\n\r\n     },\r\n     makeAuthorName(authors, insts){\r\n       let a = authors.split(\";\");\r\n       if (insts == undefined){\r\n         return authors\r\n       }\r\n       else{\r\n         let i = insts.split(\";\");\r\n         let rtv = \"\";\r\n         a.forEach((name, idx) => {\r\n           if(idx != a.length-1){\r\n             rtv += name+\"(\"+i[idx]+\");\";\r\n          }\r\n         });\r\n         return rtv;\r\n}\r\n     },\r\n     makeCoopList(coopList){\r\n       return coopList.join(\";\");\r\n     },\r\n     getValue(k, v, l){\r\n       if(l!='year'){\r\n         return k+' ('+v+')';\r\n       }else{\r\n         // v\r\n         // console.log('V', v);\r\n         return v[0]+' ('+v[1]+')';\r\n       }\r\n     },\r\n     addSearchItem(){\r\n       this.searches.push({word:\"\", type:\"AND\"})\r\n     },\r\n     openSiteDialog() {\r\n       //store site flags temporary\r\n       this.siteDialog = true;\r\n\r\n       this.tempSelected = this.selected;\r\n       // for (let site in this.sites) {\r\n       //   this.sites[site] = this[site]\r\n       // }\r\n     },\r\n     closeSiteDialog() {\r\n       //restore site flags\r\n       this.siteDialog = false;\r\n       this.selected = this.tempSelected;\r\n       // for (let site in this.sites) {\r\n       //   this[site] = this.sites[site]\r\n       // }\r\n     },\r\n\r\n    //remove code\r\n\r\n     //remove code\r\n\r\n\r\n\r\n\r\n  }\r\n}\r\n</script>\r\n\r\n<!-- Add \"scoped\" attribute to limit CSS to this component only -->\r\n<style>\r\n.v-list {\r\n   border-left: 1px solid red\r\n}\r\n.sticky {\r\n  width: 100%;\r\n  position: sticky;\r\n  min-height: 60px;\r\n  text-align: center;\r\n  button {\r\n  position: absolute;\r\n  bottom: 10px;\r\n  transform: translateX(-50%);\r\n}\r\n}\r\n</style>\r\n"]}]}