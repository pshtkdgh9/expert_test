     this.$store.dispatch('getIntegrationResult', data).then((sel) =>{\r\n        console.log('SEL', sel);\r\n        t.loading = false;\r\n        t.filterDis = false;\r\n        // sel.filters\r\n\r\n\r\n        // t.selected = sel.selected;\r\n        // console.log('SEL', sel.selected.paper);\r\n          // t.selected = sel;\r\n          // t.$forceUpdate()\r\n          // t.$refs['test'].$listeners.change();\r\n      });\r\n    },\r\n    setFlag(pp, k){\r\n      this.$store.state.filters[pp][k].f = !this.$store.state.filters[pp][k].f ;\r\n\r\n      // fil\r\n    },\r\n\r\n    refreshHistories() {\r\n      this.$store.dispatch('getHistories', 0)\r\n    },\r\n     onScroll () {\r\n\r\n     },\r\n     makeAuthorName(authors, insts){\r\n       let a = authors.split(\";\");\r\n       let i = insts.split(\";\");\r\n       let rtv = \"\";\r\n       a.forEach((name, idx) => {\r\n         if(idx != a.length-1){\r\n           rtv += name+\"(\"+i[idx]+\");\";\r\n        }\r\n       });\r\n       return rtv;\r\n\r\n     },\r\n     makeCoopList(coopList){\r\n       return coopList.join(\";\");\r\n     },\r\n     getValue(k, v, l){\r\n       if(l!='year'){\r\n         return k+' ('+v+')';\r\n       }else{\r\n         // v\r\n         // console.log('V', v);\r\n         return v[0]+' ('+v[1]+')';\r\n       }\r\n     }\r\n    //remove code\r\n\r\n     //remove code\r\n\r\n\r\n\r\n\r\n  }\r\n}\r\n</script>\r\n\r\n<!-- Add \"scoped\" attribute to limit CSS to this component only -->\r\n<style>\r\n.v-list {\r\n   border-left: 1px solid red\r\n}\r\n.sticky {\r\n  width: 100%;\r\n  position: sticky;\r\n  min-height: 60px;\r\n  text-align: center;\r\n  button {\r\n  position: absolute;\r\n  bottom: 10px;\r\n  transform: translateX(-50%);\r\n}\r\n}\r\n</style>\r\n"],"sourceRoot":"src/components"}]}