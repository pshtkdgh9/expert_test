A,KAAA;AACA,aAHA,MAIA;AACA,cAAA,IAAA,CAAA,IAAA,CAAA,UAAA,GAAA,EAAA,CAAA,EAAA;AACA,oBAAA,GAAA,CAAA,MAAA,CAAA,IAAA,SAAA,IAAA,GAAA,CAAA,sBAAA,CAAA,IAAA,SAAA,EACA;AACA,kBAAA,OAAA,CAAA,GAAA,CAAA,eAAA,EAAA,CAAA;AACA,kBAAA,IAAA,GAAA,KAAA;AACA,yBAAA,IAAA;AACA;AACA,eAPA;AAQA;;AACA,gBAAA,IAAA,EAAA;AACA,cAAA,IAAA,CAAA,MAAA,CAAA,QAAA,CAAA,cAAA,EAAA;AAAA,gBAAA,IAAA,EAAA,IAAA;AAAA,gBAAA,IAAA,EAAA;AAAA,eAAA;AACA,aAFA,MAEA;AACA,cAAA,IAAA,CAAA,MAAA,CAAA,MAAA,CAAA,WAAA,EAAA,iBAAA;AACA;AACA,WAvBA;AAwBA,SA5BA;;AA6BA,QAAA,MAAA,CAAA,OAAA,GAAA,UAAA,EAAA,EAAA;AACA,UAAA,IAAA,CAAA,MAAA,CAAA,MAAA,CAAA,WAAA,EAAA,EAAA;AACA,SAFA;;AAGA,QAAA,MAAA,CAAA,kBAAA,CAAA,KAAA,IAAA,CAAA,KAAA,CAAA;AACA;AACA;AAxCA;AARA,CAAA","sourcesContent":["<template lang=\"html\">\r\n  <v-container class=\"col-6\">\r\n    <v-row align=\"center\" justify=\"center\" >\r\n      <v-col cols='12'>\r\n        <v-card>\r\n          <v-card-title>\r\n            <span class=\"headline\">(국내)IF File 설정</span>\r\n          </v-card-title>\r\n          <v-card-subtitle class=\"align-self-start\">\r\n            (출처 : <a target=\"_black\" rel=\"noopener noreferrer\" href='https://www.kci.go.kr/kciportal/po/search/poCitaSearList.kci'>KCI</a> )\r\n          </v-card-subtitle>\r\n          <v-card-text>\r\n            <v-file-input outlined color=\"success\" label=\"KCI File Import\" v-model=\"file.kci\">\r\n            </v-file-input>\r\n          </v-card-text>\r\n          <v-card-actions>\r\n            <v-spacer></v-spacer>\r\n            <v-btn color=\"primary\" right @click=\"importFile('kci')\">Upload</v-btn>\r\n          </v-card-actions>\r\n        </v-card>\r\n      </v-col>\r\n    </v-row>\r\n    <v-row align=\"center\" justify=\"center\" >\r\n      <v-col cols='12'>\r\n        <v-card>\r\n          <v-card-title>\r\n            <span class=\"headline\">(국외)IF File 설정</span>\r\n          </v-card-title>\r\n          <v-card-subtitle class=\"align-self-start\">\r\n            (출처 : <a target=\"_black\" rel=\"noopener noreferrer\" href='https://jcr.clarivate.com/JCRJournalHomeAction.action'>SCI</a> )\r\n          </v-card-subtitle>\r\n          <v-card-text>\r\n            <v-file-input outlined color=\"success\" label=\"SCI File Import\" v-model=\"file.sci\">\r\n            </v-file-input>\r\n          </v-card-text>\r\n          <v-card-actions>\r\n            <v-spacer></v-spacer>\r\n            <v-btn color=\"primary\" right @click=\"importFile('sci')\">Upload</v-btn>\r\n          </v-card-actions>\r\n        </v-card>\r\n      </v-col>\r\n    </v-row>\r\n  </v-container >\r\n</template>\r\n\r\n\r\n<script>\r\n\r\n\r\nimport XLSX from 'xlsx'\r\n\r\nexport default {\r\n  data() {\r\n    return {\r\n      file : { sci : null, kci : null},\r\n    }},\r\n    created(){\r\n      this.$store.commit(\"setIsMini\", false);\r\n    },\r\n    methods: {\r\n      importFile(bType){\r\n\r\n        if( this.file[bType]){\r\n          let self = this;\r\n          let reader = new FileReader();\r\n          reader.onload = function() {\r\n            let data  = reader.result;\r\n            let wb = XLSX.read(data, {type : 'binary'});\r\n\r\n            wb.SheetNames.forEach((sn) => {\r\n              let rows  = XLSX.utils.sheet_to_json(wb.Sheets[sn]);\r\n              // console.log('ROWS', rows);\r\n              let flag = true;\r\n              if(bType == 'sci'){\r\n                if(rows[0].__EMPTY_1 != 'Full Journal Title' && rows[0].__EMPTY_2 !='Journal Impact Factor')\r\n                flag = false;\r\n              }\r\n              else{\r\n                rows.some((row, i) => {\r\n                  if(row['발행기관'] == undefined || row['(자기인용제외) KCI IF (5년)'] == undefined)\r\n                  {\r\n                    console.log('error line : ', i);\r\n                    flag = false;\r\n                    return true;\r\n                  }\r\n                });\r\n              }\r\n              if(flag){\r\n                self.$store.dispatch('uploadIFFile', { rows, type : bType });\r\n              }else{\r\n                self.$store.commit('showSnack', 'File Type error');\r\n              }\r\n            });\r\n          };\r\n          reader.onerror = function(ex) {\r\n            self.$store.commit('showSnack', ex);\r\n          };\r\n          reader.readAsBinaryString(this.file[bType]);\r\n        }\r\n      },\r\n    }\r\n  }\r\n\r\n  </script>\r\n"],"sourceRoot":"src/components"}]}